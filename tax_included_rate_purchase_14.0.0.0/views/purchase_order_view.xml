<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_purchase_order_form_taxincluded_rate" model="ir.ui.view">
        <field name="name">purchase.order.form.tax.included.rate</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_qty']" position="after">
                <field name="tax_included_rate" readonly="False"/>
            </xpath>
        </field>
    </record>
</odoo>